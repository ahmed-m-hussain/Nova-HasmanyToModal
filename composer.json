{
    "name": "ahmed-hussain/hasmany-to-modal",
    "description": "A Laravel Nova field. To create a modal for creating or viewing related HasMany records without leaving the index page",
    "keywords": [
        "laravel",
        "nova",
        "hasmany",
        "modal"
    ],
    "authors": [
        {
            "name": "Ahmed Hussain",
            "email": "ahmed@ahmed-hussain.com",
            "role": "Author"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "AhmedHussain\\HasmanyToModal\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AhmedHussain\\HasmanyToModal\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
